<Page x:Class="WpfApp1.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Login">
    <Grid>
     <Label x:Name="lblUsername"  HorizontalAlignment="Left" Height="29" Margin="183,145,0,0" VerticalAlignment="Top" Width="110" Content="Username" />
     <Label x:Name="lblPassword"  HorizontalAlignment="Left" Height="29" Margin="183,200,0,0" VerticalAlignment="Top" Width="110" Content="Password"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="328,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="328,200,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="328,260,0,0" VerticalAlignment="Top" Width="75" Click="BtnLogin_Click"/>


    </Grid>
    
</Page>
